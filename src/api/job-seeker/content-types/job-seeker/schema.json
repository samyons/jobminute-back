{
  "kind": "collectionType",
  "collectionName": "job_seekers",
  "info": {
    "singularName": "job-seeker",
    "pluralName": "job-seekers",
    "displayName": "Job Seeker",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Homme",
        "Femme"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "job_seeker"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education"
    },
    "workExperiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-experience.work-experience"
    },
    "job_position": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::job-position.job-position"
    }
  }
}
